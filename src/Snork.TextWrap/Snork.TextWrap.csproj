<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Copyright>(c) 2022 by Xavier Jefferson.  All rights reserved.</Copyright>
    <PackageProjectUrl>https://github.com/xavierjefferson/Snork.TextWrap</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xavierjefferson/Snork.TextWrap</RepositoryUrl>
    <PackageTags>text wrap;word wrap</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Snork.TextWrap</Title>
    <Version>1.0.1301</Version>
    <Authors>XavierJefferson</Authors>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <PackageId>Snork.TextWrap</PackageId>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
